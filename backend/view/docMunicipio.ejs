    <table>
      <tr>
        <td>Numero 1</td>
        <td>1</td>
      </tr>
      <tr>
        <td>Nombre del Servicio</td>
        <td>Crear municipios</td>
      </tr>
      <tr>
        <td>URI</td>
        <td>/v1/municipios</td>
      </tr>
      <tr>
        <td>Descripcion:</td>
        <td>Permite crear municipios en la base de datos</td>
      </tr>
      <tr>
        <td>Verbo HTTP:</td>
        <td>POST</td>
      </tr>
      <tr>
        <td>HEADER:</td>
        <td>token: token</td>
      </tr>
      <tr>
        <td>Usuarios:</td>
        <td>ADMINISTRADOR</td>
      </tr>
      <tr>
        <td>PETICION BODY</td>
        <td>
          <pre><code>{
  "pk_codigo_muni": "100",
  "nombre_muni": "nuevo municipio",
  "fk_departamento": "41"
}</code></pre>
        </td>
      </tr>
      <tr>
        <td>RESPUESTA JSON</td>
        <td>
          <strong>Correcto:</strong> Status: 200
          <pre><code>{
  "message": "municipio creado exitosamente"
}</code></pre>
          <strong>Incorrecto:</strong> Status: 400 (Municipio existente)
          <pre><code>{
  "message": "El municipio que quieres crear ya existe"
}</code></pre>
          <strong>Incorrecto:</strong> Status: 400 (Código existente)
          <pre><code>{
  "message": "El código del municipio ya existe"
}</code></pre>
          <strong>Incorrecto:</strong> Status: 404
          <pre><code>{
  "message": "No se pudo crear el municipio"
}</code></pre>
          <strong>Error:</strong> Status: 500
          <pre><code>{
  "message": "Error en el servidor: " + error
}</code></pre>
        </td>
      </tr>
      <tr>
        <td>Desarrollador:</td>
        <td>Juan Camilo Realpe Ceron</td>
      </tr>
      <tr>
        <td>Rama:</td>
        <td>DEVjcrc</td>
      </tr>
    </table>

